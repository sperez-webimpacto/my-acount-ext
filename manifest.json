  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "infra.io.vtex.com",
        "path": "*"
      }
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "read-workspace-apps"
