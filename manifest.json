    "giftcardcencosudqa.shopping-orders-administration": "0.x"
  },
  "credentialType": "absolute",
  "categories": [],
  "builders": {
    "styles": "2.x",
    "store": "0.x",
    "sitemap": "0.x",
    "react": "3.x",
    "node": "6.x",
    "graphql": "1.x",
    "docs": "0.x"
